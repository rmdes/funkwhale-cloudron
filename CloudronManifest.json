{
  "id": "audio.funkwhale.cloudronapp",
  "title": "Funkwhale",
  "author": "Funkwhale Community",
  "description": "file://DESCRIPTION.md",
  "tagline": "A self-hosted, federated music streaming platform",
  "version": "1.0.0",
  "upstreamVersion": "2.0.0-rc13",
  "healthCheckPath": "/api/v2/instance/nodeinfo/2.1/",
  "httpPort": 8000,
  "addons": {
    "localstorage": {},
    "postgresql": {},
    "redis": {},
    "sendmail": {},
    "scheduler": {
      "00_read_me_first": {
        "schedule": "0 0 30 2 *",
        "command": "echo '=== Funkwhale Scheduler Tasks ===' && echo '' && echo 'SAFE tasks (run automatically):' && echo '  clear_sessions, clear_expired_tokens, collect_static' && echo '' && echo 'ON-DEMAND tasks (yearly schedule, use via this dropdown):' && echo '  *_check tasks = DRY RUN (safe, shows what would change)' && echo '  *_apply tasks = DESTRUCTIVE (actually modifies the database)' && echo '' && echo 'TIP: Add --help to any command before running it to see options.' && echo '     e.g. /app/pkg/manage.sh prune_library --help' && echo '' && echo 'All prune/fix commands default to dry-run. You must use the' && echo '_apply variant or pass --no-dry-run to make real changes.'"
      },
      "clear_sessions": {
        "schedule": "23 3 * * *",
        "command": "/app/pkg/manage.sh clearsessions"
      },
      "clear_expired_tokens": {
        "schedule": "43 3 * * *",
        "command": "/app/pkg/manage.sh cleartokens"
      },
      "collect_static": {
        "schedule": "0 4 * * 0",
        "command": "/app/pkg/manage.sh collectstatic --noinput"
      },
      "check_preferences": {
        "schedule": "0 5 1 1 *",
        "command": "/app/pkg/manage.sh checkpreferences"
      },
      "rebuild_music_permissions": {
        "schedule": "0 5 2 1 *",
        "command": "/app/pkg/manage.sh rebuild_music_permissions"
      },
      "run_migrations": {
        "schedule": "0 5 3 1 *",
        "command": "/app/pkg/manage.sh migrate --noinput"
      },
      "prune_library_check": {
        "schedule": "0 5 4 1 *",
        "command": "/app/pkg/manage.sh prune_library --artists --albums --tracks"
      },
      "prune_library_apply": {
        "schedule": "0 5 5 1 *",
        "command": "/app/pkg/manage.sh prune_library --no-dry-run --artists --albums --tracks"
      },
      "prune_non_mbid_check": {
        "schedule": "0 5 6 1 *",
        "command": "/app/pkg/manage.sh prune_non_mbid_content"
      },
      "prune_non_mbid_apply": {
        "schedule": "0 5 7 1 *",
        "command": "/app/pkg/manage.sh prune_non_mbid_content --no-dry-run"
      },
      "prune_skipped_uploads_check": {
        "schedule": "0 5 8 1 *",
        "command": "/app/pkg/manage.sh prune_skipped_uploads"
      },
      "prune_skipped_uploads_apply": {
        "schedule": "0 5 9 1 *",
        "command": "/app/pkg/manage.sh prune_skipped_uploads --force true"
      },
      "check_inplace_files_check": {
        "schedule": "0 5 10 1 *",
        "command": "/app/pkg/manage.sh check_inplace_files"
      },
      "check_inplace_files_apply": {
        "schedule": "0 5 11 1 *",
        "command": "/app/pkg/manage.sh check_inplace_files --no-dry-run"
      },
      "fix_uploads_check": {
        "schedule": "0 5 12 1 *",
        "command": "/app/pkg/manage.sh fix_uploads --dry-run --mimetype --size --audio-data --checksum"
      },
      "fix_uploads_apply": {
        "schedule": "0 5 13 1 *",
        "command": "/app/pkg/manage.sh fix_uploads --mimetype --size --audio-data --checksum"
      }
    }
  },
  "manifestVersion": 2,
  "website": "https://funkwhale.audio",
  "contactEmail": "contact@funkwhale.audio",
  "icon": "file://logo.png",
  "tags": ["music", "streaming", "podcast", "audio", "federation", "activitypub", "subsonic"],
  "changelog": "file://CHANGELOG.md",
  "postInstallMessage": "file://POSTINSTALL.md",
  "memoryLimit": 1073741824,
  "minBoxVersion": "8.1.0"
}
